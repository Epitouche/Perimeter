{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "path": "ink-docstrap",
    "theme": "cerulean",
    "systemName": "My React Native App",
    "footer": "Generated with JSDoc",
    "copyright": "Copyright Â© 2025",
    "navType": "vertical",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": false,
    "highlightTutorialCode": true
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "./README.md",
    "encoding": "utf8"
  }
}
